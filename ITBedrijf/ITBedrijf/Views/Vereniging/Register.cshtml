@model PMOrganisationRegister
<h2>Kassa's voor @ViewBag.Organisation.OrganisationName:@ViewBag.Organisation.Login</h2>
<table>
    <tr>
        <td width="400px">@Html.LabelFor(a => a.OrganisationName):@Html.LabelFor(a => a.Login)</td>
        <td width="400px">@Html.LabelFor(a => a.RegisterName):@Html.LabelFor(a => a.Device)</td>
        <td width="200px">@Html.LabelFor(a => a.FromDate)</td>
        <td width="200px">@Html.LabelFor(a => a.UntilDate)</td>
    </tr>
    @foreach (var item in @ViewBag.Register)
    {
        <tr>
            <td>@item.OrganisationName:@item.Login</td>
            <td>@item.RegisterName:@item.Device</td>
            <td>@item.FromDate</td>
            <td>@item.UntilDate</td>
        </tr>
    }
</table>
<p><a href="/Vereniging/NewRegister?id=@ViewBag.Organisation.Id">Kassa Toevoegen</a></p>


